using System;

class Program
{
    static int Fibonacci(int n)
    {
        if (n <= 1)
            return n;
        return Fibonacci(n - 1) + Fibonacci(n - 2);
    }

    static void Main()
    {
        int number = 10; // تعداد اعضای دنباله فیبوناچی
        for (int i = 0; i < number; i++)
        {
            Console.Write($"{Fibonacci(i)} ");
        }
    }
}
